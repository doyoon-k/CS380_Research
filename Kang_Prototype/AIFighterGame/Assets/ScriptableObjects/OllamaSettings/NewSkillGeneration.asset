      enumOptions: []
      minValue: 
      maxValue: 
    - fieldName: abilityDescription
      fieldType: 0
      arrayElementType: 0
      children: []
      enumOptions: []
      minValue: 
      maxValue: 
    enumOptions: []
    minValue: 
    maxValue: 
